# A sample Bowerfile
# Check out https://github.com/42dev/bower-rails#ruby-dsl-configuration for more options
assets_path "assets/javascripts"
# Puts files under ./vendor/assets/js/bower_components
group :vendor, :assets_path => "assets/javascripts"  do
  asset "jquery"            # Defaults to 'latest'
  asset "datetimepicker"
end
# asset 'bootstrap'
