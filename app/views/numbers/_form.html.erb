<%= simple_form_for(@number) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :sub_account %>
    <%= f.input :number %>
    <%= f.input :sid %>
    <%= f.input :sauth_token %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
